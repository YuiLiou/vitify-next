    "vue-echarts": "^6.7.3",
    "vue-router": "^4.4.0",
    "vuetify": "^3.6.12"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@nuxt/eslint-config": "0.3.13",
    "@pinia/testing": "^0.1.3",
    "@playwright/test": "^1.45.1",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.10",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.6.0",
